{
  "name": "sanji-rest-ui",
  "description": "sanji-rest-ui UI",
  "version": "0.0.0-semantic-released",
  "main": "index",
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:once && npm run check-coverage"
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "NODE_ENV=production webpack --config webpack.build.js --display-modules",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 90 --branches 90 --functions 90 --lines 90",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "start": "NODE_ENV=development node server/dev-server.js",
    "test": "NODE_ENV=test karma start",
    "test:once": "karma start",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "angular-material": "~0.11.0",
    "angular-material-icons": "~0.6.0",
    "angular-mocks": "~1.4.7",
    "angular-sanji-window": "Sanji-IO/angular-sanji-window#webpack-sanji-window",
    "autoprefixer-loader": "~3.1.0",
    "babel-core": "~5.8.24",
    "babel-eslint": "~4.1.3",
    "babel-loader": "~5.3.2",
    "babel-runtime": "~5.8.25",
    "codecov.io": "~0.1.6",
    "commitizen": "~1.0.5",
    "css-loader": "~0.19.0",
    "cz-conventional-changelog": "1.1.2",
    "es5-shim": "~4.1.14",
    "es6-shim": "~0.28.1",
    "eslint": "~1.6.0",
    "eslint-loader": "~1.0.0",
    "extract-text-webpack-plugin": "~0.8.2",
    "file-loader": "~0.8.4",
    "ghooks": "~0.3.2",
    "html-webpack-plugin": "~1.6.1",
    "isparta-loader": "~1.0.0",
    "istanbul": "~0.3.22",
    "jquery": "~2.1.4",
    "karma": "~0.13.10",
    "karma-chai-plugins": "~0.6.0",
    "karma-coverage": "~0.5.2",
    "karma-es6-shim": "~0.1.3",
    "karma-mocha": "~0.2.0",
    "karma-mocha-reporter": "~1.1.1",
    "karma-phantomjs-launcher": "~0.2.1",
    "karma-sourcemap-loader": "~0.3.5",
    "karma-webpack": "~1.7.0",
    "ng-annotate-loader": "0.0.10",
    "ng-cache-loader": "0.0.13",
    "node-bourbon": "~4.2.3",
    "node-sass": "~3.3.3",
    "phantomjs": "~1.9.18",
    "raw-loader": "~0.5.1",
    "sass-loader": "~3.0.0",
    "semantic-release": "~4.3.5",
    "style-loader": "~0.12.3",
    "svg-morpheus": "alexk111/SVG-Morpheus#v0.1.8",
    "url-loader": "~0.5.6",
    "webpack": "~1.12.1",
    "webpack-dev-server": "~1.12.0",
    "webpack-notifier": "~1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sanji-IO/sanji-rest-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Sanji-IO/sanji-rest-ui/issues"
  },
  "keywords": [
    "sanji"
  ],
  "author": {
    "name": "Zack Yang",
    "email": "zack9433@gmail.com",
    "url": "https://github.com/Sanji-IO"
  },
  "license": "MIT",
  "homepage": "https://github.com/Sanji/sanji-rest-ui",
  "peerDependencies": {
    "toastr": "^2.1.2"
  },
  "dependencies": {
    "angular": "~1.4.7"
  }
}
